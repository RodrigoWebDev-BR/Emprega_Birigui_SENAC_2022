}

<h1>Excluir Vaga Beneficio</h1>

<h3>Are you sure you want to delete this?</h3>
<div>
    <h4>Vaga Beneficio</h4>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.VagaBeneficio.IdBeneficioNavigation)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.VagaBeneficio.IdBeneficioNavigation.Id)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.VagaBeneficio.IdVagaNavigation)
        </dt>
