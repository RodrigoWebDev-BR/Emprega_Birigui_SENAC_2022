@page
@model Emprega.Pages.Empresa.IndexModel

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-page="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Empresa[0].RazaoSocial)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Empresa[0].NomeFantasia)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Empresa[0].Cnpj)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Empresa[0].Cep)
            </th>
